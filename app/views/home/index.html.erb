    <table>
      <tr>
        <th><b>Name</b></th>
        <th><b>Modified</b></th>
        <th><b>Members</b></th>
      </tr>
      <tr>
        <td>Sample name</td>
        <td>Sample modified</td>
        <td>Sample members</td>
      </tr>
    </table>
  </div>
</div>
