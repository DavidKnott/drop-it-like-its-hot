<nav>
  <div class="nav-wrapper">
    <a href="#" class="brand-logo center"><%= image_tag("dropbox_logotype", class:"menu-logo") %></a>
    <ul id="nav-mobile" class="right hide-on-med-and-down"></ul>
  </div>
</nav>
<div class="test">
  <div class="row">
    <div class="col s6">
    <%= image_tag("dropbox_possibility_box.png", class:"possibility-box") %>
    </div>
    <div class="col s3 login-info ">
      <%= render partial: 'form' %>
      <%= link_to "Sign in with Google", "/auth/google_oauth2", class: "btn" %>
      <%= link_to "Create Account", new_user_path %>
      <%= form_tag login_path, method: "post" do %>
        <div class="input-field">
          <%= text_field_tag :email, nil, placeholder: "Email" %>
        </div>
        <div class="input-field">
          <%= password_field_tag :password, nil, placeholder: "Password" %>
        </div>
        <div class="input-field">
          <%= submit_tag "Sign in", class: "btn" %>
        </div>
    <% end %>
    </div>
  </div>
</div>