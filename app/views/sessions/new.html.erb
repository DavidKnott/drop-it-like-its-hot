<nav>
  <div class="nav-wrapper">
    <a href="#" class="brand-logo center"><%= image_tag("dropbox_logotype", class:"menu-logo") %></a>
    <ul id="nav-mobile" class="right hide-on-med-and-down"></ul>
  </div>
</nav>
<div class="test">
  <div class="row">
    <div class="col s6">
    <%= image_tag("dropbox_possibility_box.png", class:"possibility-box") %>
    </div>
    <div class="col s3">
      <%= link_to "Sign in with Google", "/auth/google_oauth2" %>
    </div>
    <%= form_tag login_path, method: "post" do %>
        <div class="input-field col s3">
          <%= text_field_tag :email %>
        </div>
        <div class="input-field col s3">
          <%= text_field_tag :password %>
        </div>
        <div class="input-field col s3">
          <%= submit_tag "Sign in" %>
        </div>
    <% end %>
  </div>
</div>